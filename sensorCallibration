/*
 * Terrapin
 * POE Fall 2016
 * Sensor Calibration
 */

int sensor1Pin = A2, sensor2Pin = A3;                  // select the input pins for the sensors
int sensor1Value = 0, sensor2Value = 0;                // variables to store the value coming from the sensor

void setup() {
  // Begin Serial in order to see data
  Serial.begin(9600);
}

void loop() {
  // read the value from the sensors, then print them separated by commas
  sensor1Value = analogRead(sensor1Pin);
  Serial.println(sensor1Value);
}
